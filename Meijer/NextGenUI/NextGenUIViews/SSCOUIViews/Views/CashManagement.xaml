<c:BackgroundView
    x:Class="SSCOUIViews.Views.CashManagement"
    xmlns:c="clr-namespace:SSCOUIModels.Controls;assembly=SSCOUIModels"
    xmlns:controls="clr-namespace:SSCOControls;assembly=SSCOControls"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:helpers="clr-namespace:SSCOUIModels.Helpers;assembly=SSCOUIModels"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    mc:Ignorable="d"
    xmlns:local="clr-namespace:SSCOUIViews.Controls"
    ShowCart="False"
    ShowDecorator="False"
    ShowHeader="False"
    ShowSystemFunctions="False">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition
                Height="190" />
            <RowDefinition
                Height="580" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition
                Width="699" />
            <ColumnDefinition
                Width="325" />
        </Grid.ColumnDefinitions>
        <DockPanel
            Grid.Row="0"
            Grid.Column="0"
            Margin="10,10,0,0">
            <DockPanel
                DockPanel.Dock="Top"
                Height="40">
                <controls:MeasureTextBlock
                    Name="CashManagementTitle"
                    Style="{StaticResource storeModeHeaderTitleTextStyle}" />
                <controls:ImageButton
                    Name="LockScreenButton"
                    Text="{Loc XM_LockScreen}"
                    Height="40"
                    Width="133"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Style="{StaticResource cashManagementLockScreenButtonStyle}"
                    Command="{Binding ActionCommand}"
                    CommandParameter="XMLockScreen" />
            </DockPanel>
            <controls:MeasureTextBlock
                Name="CashManagementLeadthruTitle"
                DockPanel.Dock="Top"
                HorizontalAlignment="Left"
                Margin="0,5,0,0"
                Style="{StaticResource cashManagementLeadthruTitleTextStyle}" />
            <controls:MeasureTextBlock
                Name="CashManagementLeadthru"
                DockPanel.Dock="Top"
                HorizontalAlignment="Left"
                Margin="0,8,0,0"
                Style="{StaticResource cashManagementLeadthruTextStyle}" />
            <StackPanel
                VerticalAlignment="Bottom">
                <controls:MeasureTextBlock
                    Name="CashManagementGrandTotal"
                    Margin="0,8,0,0"
                    Text="{Binding XMGrandTotalText}"
                    Style="{StaticResource cashManagementGloryTotalTextStyle}" />
                <controls:MeasureTextBlock
                    Name="CashManagementTotalChanged"
                    Margin="0,2,0,0"
                    Style="{StaticResource cashManagementGloryTotalTextStyle}" />
            </StackPanel>
        </DockPanel>
        <Grid
            Grid.Row="1"
            Grid.Column="0"
            VerticalAlignment="Bottom"
            Margin="10,0,0,10"
            Height="564">
            <Border
                Style="{StaticResource glowBorderStyle}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="34" />
                        <RowDefinition
                            Height="228" />
                        <RowDefinition
                            Height="34" />
                        <RowDefinition
                            Height="228" />
                        <RowDefinition
                            Height="*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="88" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="65" />
                        <ColumnDefinition
                            Width="*" />
                    </Grid.ColumnDefinitions>
                    <MeasureTextBlock
                        Grid.Row="0"
                        Grid.Column="0"
                        Grid.ColumnSpan="10"
                        Margin="4,4,0,0"
                        Text="{Loc XM_Coins}"
                        Style="{StaticResource cashManagementCurrencyAreaTextStyle}" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="0"
                        x:Name="NonDispenseCylinder401"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="NonDispense"
                        CylinderNumber="401"
                        Cylinder="XMNonDispenseCylinder401" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="401"
                        Cylinder="XMGenericCylinder401" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="2"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="402"
                        Cylinder="XMGenericCylinder402" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="3"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="403"
                        Cylinder="XMGenericCylinder403" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="4"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="404"
                        Cylinder="XMGenericCylinder404" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="5"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="405"
                        Cylinder="XMGenericCylinder405" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="6"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="406"
                        Cylinder="XMGenericCylinder406" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="7"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="407"
                        Cylinder="XMGenericCylinder407" />
                    <local:CurrencyCylinder
                        Grid.Row="1"
                        Grid.Column="8"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Coins"
                        CylinderType="Generic"
                        CylinderNumber="408"
                        Cylinder="XMGenericCylinder408" />
                    <StackPanel
                        Grid.Row="1"
                        Grid.Column="9"
                        Name="TotalCoinPanel"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top">
                        <Border
                            Name="TotalCoins"
                            Width="79"
                            Height="40"
                            HorizontalAlignment="Center"
                            Style="{StaticResource cashManagementPurgeCountBorderStyle}">
                            <StackPanel>
                                <MeasureTextBlock
                                    Name="TotalCoinValue"
                                    Text="{Binding XMTotalCoinValueText}"
                                    Style="{StaticResource cashManagementNonDispenseValueChangeStyle}" />
                                <MeasureTextBlock
                                    Name="TotalCoinCount"
                                    Text="{Binding XMTotalCoinCountText}"
                                    Style="{StaticResource cashManagementNonDispenseCountChangeStyle}" />
                            </StackPanel>
                        </Border>
                        <Border
                            Name="PendingCoins"
                            Width="75"
                            Height="40"
                            HorizontalAlignment="Center"
                            Visibility="Collapsed"
                            Style="{StaticResource cashManagementPurgeCountBorderStyle}">
                            <StackPanel>
                                <MeasureTextBlock
                                    Name="PendingCoinValue"
                                    Text="{Binding XMPendingCoinValueText}"
                                    Style="{StaticResource cashManagementValueChangeStyle}" />
                                <MeasureTextBlock
                                    Name="PendingCoinCount"
                                    Text="{Binding XMPendingCoinCountText}"
                                    Style="{StaticResource cashManagementCountChangeStyle}" />
                            </StackPanel>
                        </Border>
                        <MeasureTextBlock
                            Name="TotalCoinArea"
                            Height="40"
                            Margin="0,5,0,0"
                            Style="{StaticResource cashManagementTotalDescriptionStyle}" />
                        <MeasureTextBlock
                            Name="PendingCoinArea1"
                            Height="20"
                            Margin="0,5,0,0"
                            Style="{StaticResource cashManagementTotalDescriptionStyle}" />
                        <MeasureTextBlock
                            Name="PendingCoinArea2"
                            Height="20"
                            Style="{StaticResource cashManagementTotalDescriptionStyle}" />
                    </StackPanel>
                    <Separator
                        Grid.Row="2"
                        Grid.ColumnSpan="10"
                        VerticalAlignment="Top" />
                    <MeasureTextBlock
                        Grid.Row="2"
                        Grid.Column="0"
                        Grid.ColumnSpan="10"
                        Margin="4,4,0,0"
                        Text="{Loc XM_Notes}"
                        Style="{StaticResource cashManagementCurrencyAreaTextStyle}" />
                    <StackPanel
                        Grid.Row="3"
                        Grid.Column="0"
                        VerticalAlignment="Top"
                        Margin="4,4,0,0">
                        <local:CurrencyCylinder
                            CurrencyType="Notes"
                            CylinderType="NonDispense"
                            CylinderNumber="501"
                            Cylinder="XMNonDispenseCylinder501" />
                        <MeasureTextBlock
                            Name="PurgeInstructionHolder"
                            Margin="0,4,0,0"
                            Text="{Binding XMPurgeOperationHolderText}"
                            Style="{StaticResource cashManagementPurgeCountHolderStyle}" />
                        <Border
                            Name="PurgeCountHolder"
                            Style="{StaticResource cashManagementPurgeCountBorderStyle}">
                            <Canvas
                                Margin="56,0,0,0">
                                <Image
                                    Name="YieldIcon"
                                    Style="{StaticResource yieldIconStyle}">
                                </Image>
                                <MeasureTextBlock
                                    Canvas.Right="33"
                                    Name="PurgeCount"
                                    Height="Auto"
                                    Text="{Binding XMPurgeOperationCountText}"
                                    Style="{StaticResource cashManagementPurgeCountStyle}" />
                            </Canvas>
                        </Border>
                    </StackPanel>
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="1"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="501"
                        Cylinder="XMGenericCylinder501" />
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="2"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="502"
                        Cylinder="XMGenericCylinder502" />
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="3"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="503"
                        Cylinder="XMGenericCylinder503" />
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="4"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="504"
                        Cylinder="XMGenericCylinder504" />
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="5"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="505"
                        Cylinder="XMGenericCylinder505" />
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="6"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="506"
                        Cylinder="XMGenericCylinder506" />
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="7"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="507"
                        Cylinder="XMGenericCylinder507" />
                    <local:CurrencyCylinder
                        Grid.Row="3"
                        Grid.Column="8"
                        Margin="0,4,0,0"
                        VerticalAlignment="Top"
                        CurrencyType="Notes"
                        CylinderType="Generic"
                        CylinderNumber="508"
                        Cylinder="XMGenericCylinder508" />
                    <StackPanel
                        Grid.Row="3"
                        Grid.Column="9"
                        Margin="0,4,0,0"
                        Name="TotalNotePanel"
                        VerticalAlignment="Top">
                        <Border
                            Name="TotalNotes"
                            Width="79"
                            Height="40"
                            HorizontalAlignment="Center"
                            Style="{StaticResource cashManagementPurgeCountBorderStyle}">
                            <StackPanel>
                                <MeasureTextBlock
                                    Name="TotalNoteValue"
                                    Text="{Binding XMTotalNoteValueText}"
                                    Style="{StaticResource cashManagementNonDispenseValueChangeStyle}" />
                                <MeasureTextBlock
                                    Name="TotalNoteCount"
                                    Text="{Binding XMTotalNoteCountText}"
                                    Style="{StaticResource cashManagementNonDispenseCountChangeStyle}" />
                            </StackPanel>
                        </Border>
                        <Border
                            Name="PendingNotes"
                            Width="75"
                            Height="40"
                            HorizontalAlignment="Center"
                            Visibility="Collapsed"
                            Style="{StaticResource cashManagementPurgeCountBorderStyle}">
                            <StackPanel>
                                <MeasureTextBlock
                                    Name="PendingNoteValue"
                                    Text="{Binding XMPendingNoteValueText}"
                                    Style="{StaticResource cashManagementValueChangeStyle}" />
                                <MeasureTextBlock
                                    Name="PendingNoteCount"
                                    Text="{Binding XMPendingNoteCountText}"
                                    Style="{StaticResource cashManagementCountChangeStyle}" />
                            </StackPanel>
                        </Border>
                        <MeasureTextBlock
                            Name="TotalNoteArea"
                            Height="40"
                            Margin="0,5,0,0"
                            Style="{StaticResource cashManagementTotalDescriptionStyle}" />
                        <MeasureTextBlock
                            Name="PendingNoteArea1"
                            Height="20"
                            Margin="0,5,0,0"
                            Style="{StaticResource cashManagementTotalDescriptionStyle}" />
                        <MeasureTextBlock
                            Name="PendingNoteArea2"
                            Height="20"
                            Style="{StaticResource cashManagementTotalDescriptionStyle}" />
                    </StackPanel>
                    <Separator
                        Grid.Row="4"
                        Grid.ColumnSpan="10"
                        VerticalAlignment="Top" />
                    <MeasureTextBlock
                        Grid.Row="4"
                        Grid.ColumnSpan="10"
                        Margin="0,3,0,0"
                        Text="{Binding XMPurgeOperationInstructionText}"
                        Style="{StaticResource cashManagementPurgeInstructionsStyle}">
                    </MeasureTextBlock>
                </Grid>
            </Border>
        </Grid>
        <DockPanel
            Grid.Row="0"
            Grid.Column="1"
            Grid.RowSpan="2"
            Margin="10">
            <StackPanel
                DockPanel.Dock="Top"
                Name="CashStatusButtonPanel">
                <controls:ImageButton
                    Text="{Loc XM_Print}"
                    Height="64"
                    Width="305"
                    Style="{StaticResource cashManagementButtonStyle}"
                    Command="{Binding ActionCommand}"
                    CommandParameter="XMButton1"
                    IsEnabled="{Binding XMButton1Enabled}" />
                <StackPanel
                    Orientation="Horizontal"
                    Margin="0,10,0,0">
                    <controls:ImageButton
                        Name="AddCashButton"
                        Text="{Loc XM_CashReplenishment}"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMButton2" 
                        IsEnabled="{Binding XMButton2Enabled}" />
                    <controls:ImageButton
                        Text="{Loc XM_CashRemove}"
                        Height="54"
                        Width="147"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Margin="10,0,0,0"
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMButton3"
                        IsEnabled="{Binding XMButton3Enabled}" />
                </StackPanel>
            </StackPanel>
            <StackPanel
                DockPanel.Dock="Top"
                Name="MenuButtonPanel"
                Visibility="Collapsed"> 
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="*" />
                        <ColumnDefinition
                            Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="58" />
                        <RowDefinition
                            Height="58" />
                    </Grid.RowDefinitions>
                    <controls:ImageToggleButton
                        Grid.Row="0"
                        Grid.Column="0"
                        Name="MenuButton1"
                        Text="{Loc XM_MenuDispenseFunctions}"
                        Height="54"
                        Width="147"
                        VerticalAlignment="Top"
                        Style="{StaticResource cashManagementToggleButtonStyle}"
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMMenuButton5001" />
                    <controls:ImageToggleButton
                        Grid.Row="0"
                        Grid.Column="1"
                        Name="MenuButton2"
                        Text="{Loc XM_MenuResetFunctions}"
                        Height="54"
                        Width="147"
                        VerticalAlignment="Top"
                        Style="{StaticResource cashManagementToggleButtonStyle}"
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMMenuButton5002" />
                    <controls:ImageToggleButton
                        Grid.Row="1"
                        Grid.Column="0"
                        Name="MenuButton4"
                        Text="{Loc XM_MenuPickUpFunctions}"
                        Height="54"
                        Width="147"
                        VerticalAlignment="Top"
                        Style="{StaticResource cashManagementToggleButtonStyle}" 
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMMenuButton5004" />
                    <controls:ImageToggleButton
                        Grid.Row="1"
                        Grid.Column="1"
                        Name="MenuButton3"
                        Text="{Loc XM_MenuCashBoxTransfer}"
                        Height="54"
                        Width="147"
                        VerticalAlignment="Top"
                        Style="{StaticResource cashManagementToggleButtonStyle}"
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMMenuButton5003" />
                </Grid>
                <Rectangle
                    Margin="0,2,0,0"
                    Style="{StaticResource cashManagementButtonListSeparatorStyle}" />
            </StackPanel>
            <StackPanel
                DockPanel.Dock="Top"
                Name="ActionButtonPanel"
                Margin="0,2,0,0"
                Visibility="Collapsed">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="*" />
                        <ColumnDefinition
                            Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="58" />
                        <RowDefinition
                            Height="58" />
                    </Grid.RowDefinitions>
                    <controls:ImageButton
                        Grid.Row="0"
                        Grid.Column="0"
                        Name="ActionButton1"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}" />
                    <controls:ImageButton
                        Grid.Row="0"
                        Grid.Column="1"
                        Name="ActionButton2"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}" />
                    <controls:ImageButton
                        Grid.Row="1"
                        Grid.Column="0"
                        Name="ActionButton3"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}" />
                    <!--+ SSCOI-45868-->
                    <controls:ImageButton
                        Grid.Row="1"
                        Grid.Column="1"
                        Name="ActionButton4"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}" />
                    <!-- SSCOI-45868-->
                    <controls:ImageButton
                        Grid.Row="1"
                        Grid.Column="1"
                        Name="ActionButton5"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}" />
                </Grid>
                <Rectangle
                    Margin="0,2,0,0"
                    Style="{StaticResource cashManagementButtonListSeparatorStyle}" />
            </StackPanel>
            <StackPanel
                DockPanel.Dock="Top"
                Name="ConfirmChangesPanel"
                Margin="0,2,0,0"
                Visibility="Collapsed">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="*" />
                        <ColumnDefinition
                            Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="58" />
                    </Grid.RowDefinitions>
                    <controls:ImageButton
                        Grid.Column="0"
                        Name="ApplyChangesButton"
                        Text="{Loc XM_Execute}"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMButton1"
                        IsEnabled="{Binding XMButton1Enabled}" />
                    <controls:ImageButton
                        Grid.Column="1"
                        Name="CancelChangesButton"
                        Text="{Loc XM_CancelChanges}"
                        Height="54"
                        Width="147"
                        HorizontalAlignment="Left"
                        Style="{StaticResource cashManagementButtonStyle}"
                        Command="{Binding ActionCommand}"
                        CommandParameter="XMCancelChanges"
                        IsEnabled="{Binding XMCancelChangesEnabled}" />
                </Grid>
                <local:NumericKeypad
                    x:Name="NumericPad"
                    Margin="0,4,0,0" 
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    BackgroundViewMode="True" />
            </StackPanel>
            <controls:ImageButton
                Name="ExitButton"
                VerticalAlignment="Bottom"
                Height="64"
                Width="305"
                Margin="0,10,0,0"
                Text="{Loc XM_Exit}"
                Style="{StaticResource buttonGoBackStyle}"
                Background="{StaticResource storeModeMainButtonBackgroundColorBrush}"
                Click="ExitButton_Click"/>
        </DockPanel>
        <Canvas
            Name="AlertBoxControl"
            Grid.Column="0"
            Height="144"
            Width="689"
            Margin="10,50,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top">
            <MeasureTextBlock
                Name="AlertBoxText"
                Height="144"
                Width="689"
                Style="{StaticResource cashManagementAlertBoxStyle}">
            </MeasureTextBlock>
            <Image
                Margin="8,8,0,0"
                Style="{StaticResource alertExclamationImageStyle}">
            </Image>
        </Canvas>
        <ItemsControl
            Visibility="Hidden"
            ItemsSource="{Binding XMNumericKeyPad}" >
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <Grid>
                        <controls:ImageButton
                            Name="XMNumericKey"
                            IsEnabled="{Binding IsEnabled}"
                            IsEnabledChanged="XMNumericKey_IsEnabledChanged"
                            Loaded="XMNumericKey_Loaded" />
                    </Grid>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </Grid>
</c:BackgroundView>