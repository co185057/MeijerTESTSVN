                            Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="68" />
                        <RowDefinition
                            Height="68" />
                        <RowDefinition
                            Height="68" />
