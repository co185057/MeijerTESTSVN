        </Control>
        <Control Name="EchoAreaBackground">
          <Properties>
            <Property Name="Position">14,74,373,77</Property>
            <Property Name="Z-Order">3</Property>
          </Properties>
        </Control>
        <Control Name="LaneReceiptBackground">
          <Properties>
            <Property Name="Position">14,151,487,179</Property>
          </Properties>
        </Control>
        <Control Name="NoSaleButton">
        </Control>
        <Control Name="LaneViewWhiteBackground">
