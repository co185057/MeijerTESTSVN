<Display Version="1.0">
  <Includes>
    <!-- FULL ASSISTMODE XML -->
    <Include File="%ConfigPath%\HHACSAssistMode.xml" />
    <Include File="%ConfigPath%\HHACSAssistModeSpecific.xml" />
  </Includes>
  <Controls>
    <Control Name="LaneReceipt" Type="">
      <CustomData>
        <Data Name="ItemType38">ITEM_MSG,ITEM_DESCRIPTION</Data>
        <Data Name="ItemType38StringID">$LaneReceiptItemType2$</Data>
        <Data Name="ItemType38Selectable">True</Data>
        <Data Name="ItemType38Color">Red</Data>
        <Data Name="ItemType38Wrap">True</Data>
        <Data Name="ItemType39">ITEM_MSG,ITEM_DESCRIPTION,,ITEM_VOID</Data>
        <Data Name="ItemType39Font">ReceiptFontVoid</Data>
        <Data Name="ItemType39Color">DarkGray</Data>
        <Data Name="ItemType39Selectable">False</Data>
        <Data Name="ItemType39StringID">$LaneReceiptItemType2$</Data>
        <Data Name="ItemType39Wrap">True</Data>
      </CustomData>
    </Control>
    <Control Name="AssistReceipt" Type="">
      <CustomData>
        <Data Name="ItemType36">ITEM_MSG,ITEM_DESCRIPTION</Data>
        <Data Name="ItemType36StringID">$LaneReceiptItemType2$</Data>
        <Data Name="ItemType36Selectable">True</Data>
        <Data Name="ItemType36Color">Red</Data>
        <Data Name="ItemType36Wrap">True</Data>
        <Data Name="ItemType37">ITEM_MSG,ITEM_DESCRIPTION,,ITEM_VOID</Data>
        <Data Name="ItemType37Font">ReceiptFontVoid</Data>
        <Data Name="ItemType37Color">DarkGray</Data>
        <Data Name="ItemType37Selectable">False</Data>
        <Data Name="ItemType37StringID">$LaneReceiptItemType2$</Data>
        <Data Name="ItemType37Wrap">True</Data>
      </CustomData>
    </Control>
    <Control Name="NoSaleButton" Type="Button">
      <Properties>
        <Property Name="Position">392,9,108,63</Property>
        <Property Name="Font">LaneStatusFont</Property>
        <Property Name="TextColor">Black</Property>
        <Property Name="TextFormat">$NoSaleButtonText$</Property>
        <Property Name="Audio">%AudioPath%\BttnValid.wav</Property>
        <Property Name="TextPosition">7,7,100,49</Property>
        <Property Name="Picture">%MediaPath%\10x7-btn_toprow_lane-lg.png</Property>
        <Property Name="ClickMove">3</Property>
        <Property Name="Z-Order">2</Property>
      </Properties>
    </Control>
  </Controls>
  <Contexts>
    <Context Name="StartTransactionAM">
      <Include>
        <Control Name="HHAssistModeButton">
          <Properties>
            <Property Name="Visible">False</Property>
          </Properties>
        </Control>
      </Include>
    </Context>
    <Context Name="SmAssistMenu">
      <Include>
        <Control Name="SelectedItem">
          <Properties>
            <Property Name="Action">ChangeText,ChangeState,SMButton2,Text;ChangeText,ChangeState,SMButton3,Text</Property>
          </Properties>
        </Control>
        <Control Name="SMButton3">
          <Properties>
            <Property Name="TextFormat">$ChangeItemButton$</Property>
            <Property Name="Visible">True</Property>
          </Properties>
        </Control>
        <Control Name="SMButton4">
          <Properties>
            <Property Name="TextFormat">$MoreButton$</Property>
            <Property Name="Visible">True</Property>
          </Properties>
        </Control>
      </Include>
    </Context>
    <Context Name="ApproveAgeCheck">
      <Include>
        <Control Name="NumericKeyPad3">
          <Properties>
            <Property Name="Position">247,285,96,38</Property>
          </Properties>
        </Control>
        <Control Name="ButtonCommand1">
          <Properties>
            <Property Name="TextFormat">$DateOfBirthCancelButtonText$</Property>
            <Property Name="Position">386,156,110,58</Property>
            <Property Name="Z-Order">3</Property>
          </Properties>
        </Control>
      </Include>
    </Context>
    <Context Name="LaneStatus">
      <Include>
        <Control Name="ButtonCommand3">
