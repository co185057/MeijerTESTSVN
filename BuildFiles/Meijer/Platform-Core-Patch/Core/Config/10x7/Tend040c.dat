;For Credit/Debit only system, the tendertype option of [button8] is set to 0 to hide "Debit With Cash Back" button.
; TAR 202035 Note that the EBT cashback amounts are only used if the
; tender type is TB_TENDER_EBT or TB_TENDER_PIP_EBT.  All other tenders
; use the CashBack amounts if cashback is allowed

[button1]
TenderType = 1
Text = "Espèces"
Bitmap = "10x7_EuroCash.jpg"
CashBackIfVoided=N
PrintReceiptTrxOverAmount=0

[button2]
TenderType = 2
Text = "Carte de Crédit"
Bitmap = "10x7_creditcard.jpg"
CashBack = N
ConfirmTotal = N
NeedPin = N
IntegratedMSR = N
CaptureSignature = N
VerifySignature = N
VerifySignatureOverAmount=-1
CashBackIfVoided=N
PrintReceiptTrxOverAmount=0
Animation=swipe

