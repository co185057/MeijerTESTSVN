<Display Version="1.0">
  <Controls>
    <Control Name="SwipeCardVideo" Type="Video" Synchronize="False">
      <Properties>
        <Property Name="Video">%MediaPath%\InsertCard_40.avi</Property>
      </Properties>
    </Control>
    <Control Name="RequestSigVideo" Type="Video" Synchronize="False">
      <Properties>
        <Property Name="Video">%MediaPath%\SignName_Topaz_40.avi</Property>
      </Properties>
    </Control>
    <Control Name="TakeCardVideo" Type="Video" Synchronize="False">
      <Properties>
        <Property Name="Position">215,145,573,323</Property>
        <Property Name="Video">%MediaPath%\TakeCard_40.avi</Property>
      </Properties>
    </Control>
  </Controls>
  <Contexts>
    <Context Name="TakeCard" Synchronize="False">
      <Include>
        <Control Name="TakeCardVideo">
        </Control>
        <Control Name="InstructionTextArea">
          <CustomData>
            <Data Name="StringId">0</Data>
          </CustomData>
        </Control>
      </Include>
    </Context>
  </Contexts>
</Display>