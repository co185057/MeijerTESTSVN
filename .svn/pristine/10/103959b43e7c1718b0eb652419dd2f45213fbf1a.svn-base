; When changing this option, xml files included in FastLane.000 must be changed with the new assistmode name
; and FastLane must be restarted (it won't work if you reload options). 
AssistMode=wSMKBDIBM4680
;-----------------------------------------------
AlphaNumeric=wCMAlphaNumericKeyboard

; In-Store Pager Support
[Paging]
; Send Page Parameters (Y - send page, N - do not send page)
GreenFlashingSendPage=N
YellowSendPage=N
RedSendPage=N
OutOfServiceSendPage=N
;
; Page Resend Parameters (valid values >=0, =0 - don't resend, >0 - Elapse time in secs.)
GreenFlashingElapseTime=0
YellowElapseTime=0
RedElapseTime=90
OutOfServiceElapseTime=0
;
; Media Low Page Limits (valid values >=0, GreenFlashingLimitTime takes precedence)
GreenFlashingLimitTime=30    ;(=0 - use GreenFlashinLimitTrans, >0 - Limit in minutes)
GreenFlashingLimitTrans=15   ;(=0 - use GreenFlashinLimitTime, >0 - Limit by # of Trans.)
;
; Availability Parameters ( valid values >=0 )
AvailabilityCount=2147483647

;
; Data files
[Data]
MessageFile=SCOTMSGE
BitmapFile=SCOTBMPE
WaveFile=SCOTWAVE
SASIActionFile=SASIActione
TenderFile=SCOTTEND
TareFile=SCOTTARE
XMLFile=FastLane3.1
DefaultImagePath="%APP_DRIVE%\scot\image\Image not available_Grey_02.png"
;

[PrintObjects]
; each entry will have object details
; for a bitmap, format of key value: "bitmap name,Alignment,Width(unused as of now),scaling factor" 
ChitBmp1=%APP_DRIVE%\scot\image\scotlogo.bmp,3,0,1.000
ChitBmp2=%APP_DRIVE%\scot\image\scotlogo.bmp,3,0,1.000
;
; For slimmer receipt 58mm paper, use width=280 and height=210 for signature report
; Default for 80mm is 400x300
SignatureWindowWidth=400
SignatureWindowHeight=300

;For Topaz Signature Bitmap/Boldness : 
; Valid values are: 1, 3, 7, 15, 31, 63, 127, 255 (default value is 7)
; Table value to bits:
;    1 = 1
;    3 = 11   
;    7 = 111
;   15 = 1111
;   31 = 11111
;   63 = 111111
;  127 = 1111111
;  255 = 11111111
TopazSignatureBoldness=7

; for CM Receipt CharacterLimit and JustifyIndex Configuration
; Default Values:
; CharacterLimit=44
; JustifyIndex=27
; for 58mm receipts, configured values are CharacterLimit=35 and JustifyIndex=22
CharacterLimit=44
JustifyIndex=27

;Consumer Marketing options	
[CM]
; if 'RewardInfo' is set to 'N', no other options in this section will be considered
RewardInfo=N
; set option, 'CustomerMessage' to -1 if no message has to be displayed
; otherwise, set it to the message id which is defined in SCOTMSGE files
CustomerMessage=775
TotalSavings=Y
TotalRewardPoints=Y
; Set below option to 'N' to make TotalSavings appear on right side of reward_info bar
TotalSavingsOnLeft=Y

;This section is used to allow the store to Zero-out the variable part
;of In-Store barcodes before sending it to Security Agent. It allows Security Agent to keep
;from populating the WLDB with Barcodes whose item numbers are constant, but the rest 
;of the barcode varies because of weight, week/month ...
;the In-Store barcodes are expected to be in the format : xxiiiippppp or xxiiiivppppc
;where xx represents the barcode prefix "24" for example, iiii represents the item code
;"1234567" for example, and ppppp represents the variable part of the barcode that should be
;zeroed-out before being sent to Security Agent, "v" can be a verifier of some type,
;"c" is the checksum. In any case, the variable part "pppp", the verifier "v"(if present), the
;checksum "c"(if present) of a barcode matching a pattern defined in [InStoreBarcodes] will be replaced
;by zeroes for Security Agent only.
;A field to identify this barcode as price embedded can be added to the pattern by appending a ",Y" to the pattern.
;Note there is no limit on the number of custom codes that can be defined, but it is required that 
;they are kept in sequence: customcode1, customcode2, customcode3....
[InStoreBarcodes]
;CustomCode1=25iiiiiipppp
;CustomCode2=26iiiipppppp
;CustomCode3=27iiiiivppppc
;CustomCode4=
;CustomCode5=
;Sample with Price Embedded field set to Y
;CustomCode1=02ipppppppppc,Y


;This section is used to define Multi-Label Barcodes. Each field must be separated by white space.
;For each barcode that makes up the multi-label barcode two fields are required. The first field defines the
;OPOS compliant symbology type of the barcode. Common values are UPCA 101, UPCE 102,
;JAN8 and EAN8 104, Code39 108 and Code128 128. Other values are defined in OPOSSCAN.H.
;The next field defines the prefix of the barcode and where the item code is located. Any character 
;until the first 'x' or 'i' is read while be considered a part of the prefix.
;The barcode specific fields must appear in item code order. (In case the item code can span
;multiple barcodes.)
;An 'i' represents the item code. An 'x' represents additional characters in the barcode. (Trailing 'x's are ;optional.)
;Note: There is no limit on the number of multi-Label barcodes that can be defined (although the 
;leading prefix must be unique), but it is required that 
;they are defined in sequence: MultiLabelFormat1, MultiLabelFormat2, MultiLabelFormat3....
;
;Note: Do not define unless your scanner is properly programmed to read Multi-Label Barcodes!
;
[MultiLabelBarCodes]
;MultiLabelFormat1=104 25iiiiiiiiiix 104 28xxxxxxxxxxx
;MultiLabelFormat2=104 21iiiiiiiiii  104 22x 
;MultiLabelFormat3=104 26iiiii       104 27xxxxxiiiii
;Option to prepend first prefix to item code
PrependFirstPrefixToItemCode=N


[AutoRun]
;Set On=Y to enable AutoRun. 
;DO NOT ENABLE IN LIVE ENVIRONMENT! LAB ONLY!
On=N
;Number of transactions to run. If 0, it forces the lane to run trxs continuously (RFC375580)
;Re-start by reloading options or reloading scotapp.
NumberOfTransactionsToRun=25
;Select random timer for AttractDelay and ItemPurchaseDelay.  If select random timer for these two timers, set it to Y.  If select fix timer then set to N.
RandomTimer=Y
;maximum Number of seconds to wait at attract before starting next transaction. 
;Actual value is set randomly from 1 to the value set below (RFC375570)
AttractDelay=1
;maximum Number of seconds to wait on scan and bag before selling an item
;Actual value is set randomly from 1 to the value set below (RFC375570)
ItemPurchaseDelay=2
;Bypass security bagging logic 
;When Y, lane will ignore bag scale changes. Unexpected weight increases and decreases will not interrupt the operation.
BypassBagging=Y
;Item code to sell at scan and bag
ItemCodeToSell=1111
;Number of items to sell at a scan and bag
NumberOfItemsToSell=5
;How much cash to insert when all items are sold(quick pay)
TotalCashToPay=1585
;Set On to enable automatic remote assist mode cash tendering
;DO NOT ENABLE IN LIVE ENVIRONMENT! LAB ONLY!
AssistModeOn=N
;List of keys to send to TB while in remote assist(use AssistKeyData.dat for reference)
AssistKeyList=24 24 24 24 45 8 26 24 18 6


[Utility]
;Disable the Exit button? And set the password.
;Y: Yes N: No
ExitButtonVisible=Y
ExitPassword=1               
ExitID=1
; Training doc to show in store mode utility menu.  REPLACE/OVERRIDE WITH REAL DOCUMENT!
; Note that the flag TrainingProgramButtonVisible must be set to Y
TrainingDocument=c:\TrainingHTMLFile.htm
; Path to internet explorer executable, since it's not in the PATH variable and should not
; be hard-coded
InternetExplorerEXE=%OS_DRIVE%\program files\internet explorer\iexplore.exe

;industry standard coupon codes, should be left in unless there is a need to customise
;"x" matches any character
;"c" matches any character, but signifies check digit
; ì+î is a variable length indicator field. Use to indicate length >= length of mask
[Coupons]
CustomCoupon1=5xxxxxxxxxc
CustomCoupon2=5xxxxxxxxxxc
CustomCoupon3=99xxxxxxxxxxc
CustomCoupon4=8110xxxxxxxxxxxxxxxxxxxxx+
[Connections]
;Port# required for UDP server
UDPServerPort = 6982
;Port# required for Pipeserver
PipeserverPortno = 5051

[BarcodePrintWidths]
;Format is barcode type = barcode print width
;A value of -1 for the print width is the default (legacy behavior where the SSCO app calculates the print width)
;PTR_BCS_Codabar is 107
107=-1
;PTR_BCS_Code128 is 110
110=-1
