Private Sub CustomizeCaptions()

On Error GoTo Err_CustomizeCaptions
    '
    ' Attempt to load a configuration file containing customizations
    ' If the file is not loaded leave the defaults
    '
    Dim strFile As String: strFile = App.Path + "\Keyboard.exe.xml"
    
    Dim oDoc As New MSXML2.DOMDocument40
    Dim oKeys As MSXML2.IXMLDOMNodeList
    Dim oKey As MSXML2.IXMLDOMNode
        
    If oDoc.Load(strFile) Then
    
        Set oKeys = oDoc.selectNodes("/ACSKeyboardLayout/Key")
        
        For Each oKey In oKeys
            Dim id As Integer: id = CInt(Trim(CStr(oKey.Attributes.getNamedItem("Id").nodeValue)))
            Dim strCap As String: strCap = Trim(CStr(oKey.Attributes.getNamedItem("Caption").nodeValue))
            
            If (Command1.LBound <= id) And (id <= Command1.UBound) Then
                Command1(id).Caption = strCap
            End If
        Next oKey
    End If
    

Exit Sub
