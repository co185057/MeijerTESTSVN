Option Explicit

Private Declare Function FindWindow Lib "user32" Alias "FindWindowA" _
    (ByVal lpClassName As String, ByVal lpWindowName As String) As Long

Public WithEvents otbc1 As CtbConnect
Attribute otbc1.VB_VarHelpID = -1

Private oKey As CKeyboardObj

Private Sub Command2_Click()
    Dim msg As Object
    Set msg = CreateObject("tbControls.CashDrawerObj")
    msg.Station = DRAWER_A
    msg.Status = CD_CLOSED
    otbc1.Send msg
End Sub

Private Sub Command3_Click()
    Dim msg As Object
    Set msg = CreateObject("tbControls.CashDrawerObj")
    msg.Station = DRAWER_A
    msg.Status = CD_OPEN
    otbc1.Send msg
End Sub



Private Sub miReceiptPrinter_Click()
    miReceiptPrinter.Checked = Not miReceiptPrinter.Checked
    
    frmReceiptPrinter.Visible = miReceiptPrinter.Checked
    
    
End Sub

Private Sub otbc1_ReceiveCashDrawerObj(ByVal pCashDrawer As CCashDrawerObj)
    If (pCashDrawer.Status = CD_OPEN) Then
        Text1.Text = "CD: OPEN"
    End If
    If (pCashDrawer.Status = CD_CLOSED) Then
        Text1.Text = "CD: CLOSED"
    End If
End Sub

Private Sub Command1_Click(Index As Integer)
    oKey.KeyEvent = KEY_UP
    oKey.CombinationKeys = KEY_PRESSED_NONE
    oKey.KeyCode = CInt(Command1(Index).Tag)
    otbc1.Send oKey
End Sub

Private Sub Connect_Click()
    otbc1.Connect
End Sub

Private Sub Form_Load()
    Dim lServer As Long
    Dim nRetVal As Integer
    
    ' Let's check first if the pipe server is running...
    lServer = FindWindow(vbNullString, "pipeServer")
    If lServer <> 0 Then
        Set otbc1 = New CtbConnect
        Set oKey = New CKeyboardObj
        
        otbc1.SubscribeTo "54"
        otbc1.SetAck 0
        otbc1.Connect
        
    Text1.Text = "CD: CLOSED"
    
    ' if the pipe server is not yet running, then end this application.
    Else
        nRetVal = MsgBox("Unable to connect to the pipe server." _
            & " Please run the pipe server first.", vbExclamation)
        End
    End If
    
    CustomizeCaptions
End Sub

