<?xml version="1.0"?>
<RapNet>
	<!-- This is the list of connected FastLanes -->
	<Lanes>
	    <Lane>
	        <Host></Host>
	    </Lane>
	    <Lane>
	        <Host></Host>
	    </Lane>
	    <Lane>
	        <Host></Host>
	    </Lane>
        <Lane>
	        <Host></Host>
	    </Lane>	   
	</Lanes>
	
	<Options>
	    <!-- Use Alphanumeric keyboard for signon -->
	    <AlphaNumericKeyboard>false</AlphaNumericKeyboard>

	    <!-- EnterId screen initially uppercase -->
	    <EnterIdUppercase>false</EnterIdUppercase>

	    <!-- EnterPassword screen initially uppercase -->
	    <EnterPasswordUppercase>false</EnterPasswordUppercase>
	    
	    <!-- Timeout in ms for signon operation -->
	    <SignOnTimeout>5000</SignOnTimeout>
	    
	    <!-- Time in ms between Psx ConnectRemote invocations -->
	    <ConnectionPeriod>5000</ConnectionPeriod>
	    
	    <!-- Timeout in ms for ConnectRemote operation -->
	    <ConnectRemoteTimeout>5000</ConnectRemoteTimeout>
	    
	    <!-- Synchronize our local time with FastLane at signon -->
	    <SynchronizeTimeServer>false</SynchronizeTimeServer>

	    <!-- Sound played when a barcode is scanned. Silent if empty. -->
	    <ScanSound>Scan.wav</ScanSound>

	    <!-- Sound played when an immediate intervention is received. Silent if empty. -->
	    <ImmediateInterventionSound>SecurityHigh.wav</ImmediateInterventionSound>

	    <!-- Sound played when a delayed intervention is received. Silent if empty. -->
	    <DelayedInterventionSound>SecurityLow.wav</DelayedInterventionSound>

	    <!-- Delay in ms before an intervention sound is played. -->
	    <InterventionSoundDelay>2000</InterventionSoundDelay>

	    <!-- Allow the Alt-F4 keystroke entry to exit the application? (default is true) -->
	    <AltF4ExitAllowed>true</AltF4ExitAllowed>
	    
	    <!-- Display total transaction count of all lanes -->
		<DisplayTransactionCount>true</DisplayTransactionCount>
		
		<!-- Time interval in minutes to evaluate the total transaction count -->
		<TimePeriodForThroughputCalculation>60</TimePeriodForThroughputCalculation>
		
		<!-- Transaction count threshold for high alert-->
		<TrxCountThresholdForHighAlert>20</TrxCountThresholdForHighAlert>
		
		<!-- Transaction count threshold for caution -->
		<TrxCountThresholdForCaution>40</TrxCountThresholdForCaution>
		
		<!-- Has a Biometric device? (default is false)  --> 
		<BiometricDeviceConfigured>false</BiometricDeviceConfigured> 
	</Options>	
</RapNet>
