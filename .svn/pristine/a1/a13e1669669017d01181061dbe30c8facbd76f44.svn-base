[operations]
OPERATOR=101
PASSWORD=1

OPERATOR_TRAIN=900
PASSWORD_TRAIN=1

[TB]
TBDataEntryINI=C:\SCOT\config\ACSScotDataEntry.ini
AppIniPath=C:\ACS\DATA\APPINI.070
 
;place any key names that should not be allowed in assistmode seperated by semicolons
AssistModeDisallowedKeys=Print All Susp Trans; Transaction Suspend;Return Transaction;Refund Transaction;Frequent Buyer Lookup;Sign-Out;Enter Secure Mode;No Sale;Supervisor Functions;On-Break;Change Password;Reload Printer Logo;House Charge Balance;Change Cashier;Balance Inquiry;Duplicate EFT Receipt;Lane Return;CASH;CHECKS;ELEC. PYMT SELECTED;KEY ENTRY OF CARDS;TRAVELER/REBATE/ETC;TRAVELER/REBATE/  ETC;GUEST REQUEST CREDIT;Cancel Tender;CANADIAN CURRENCY;LAYAWAY VOUCHER;CHECK FEE REFUND;COMMERICAL CHARGE;COMMERCIAL CHARGE;BOTTLE REFUND;MISC CREDIT;PAPER FOOD STAMPS;

;place any key names that should not be allowed in remote assistmode seperated by semicolons
AssistModeDisallowedKeys_Remote=Print All Susp Trans; Transaction Suspend;Return Transaction;Refund Transaction;Frequent Buyer Lookup;Sign-Out;Enter Secure Mode;Supervisor Functions;On-Break;Change Password;Reload Printer Logo;House Charge Balance;Change Cashier;Balance Inquiry;Duplicate EFT Receipt;Lane Return;

;place any key names that should not be allowed in limited assistmode seperated by semicolons
AssistModeDisallowedKeys_Limited=Transaction Suspend; Transaction Resume; Transaction Cancel; Transaction Discount; Gift Receipt; Issue Rain Check; More; Return Transaction;Refund Transaction;Frequent Buyer Lookup;Sign-Out;Enter Secure Mode;No Sale;Supervisor Functions;On-Break;Change Password;Reload Printer Logo;House Charge Balance;Change Cashier;Balance Inquiry;Duplicate EFT Receipt;Lane Return;CASH;CHECKS;ELEC. PYMT SELECTED;KEY ENTRY OF CARDS;TRAVELER/REBATE/ETC;TRAVELER/REBATE/  ETC;GUEST REQUEST CREDIT;Cancel Tender;LAYAWAY VOUCHER; CHECK FEE REFUND;COMMERICAL CHARGE;COMMERCIAL CHARGE;BOTTLE REFUND;MISC CREDIT;PAPER FOOD STAMPS;

; 12005 - corresponds to TB_NEEDASSISTMODE
UnhandledMessageTimeoutInSeconds=6
UnhandledReturnCode=12005

;do we want to play the wave file to ask customer to set voided items aside
AskCustomerToSetAsideCancelItems=1

;do we want to allow the customer to cancel EFT transactions up until last PinPad Entry?
AllowLateEFTCancel=Y

;do we want to convert all UPC-A scans to be EAN-13 scans (driven by handheld scanner)?
ConvertUPCtoEAN=Y

;do we want to show the TBText Area for TB_SENDMESSAGETOSCOT returns?
ShowCMTBText=N
ShowSMTBText=N

;Fastlane configuration - three options: Standard, Mini, and SuperMini. Default value is Standard.
;FastLaneConfiguration=Standard
FastLaneConfiguration=Mini
;FastLaneConfiguration=SuperMini

ExpiryDuration=90
EnableBagScale=Y

HHScannerTimeOut=15

;Picklist Light weight item feature.
;Example: PicklistLightweightItems=8058;8059;71928345513
PicklistLightweightItems=

[ACSHookManager]
ApplicationModel=C:\scot\config\ApplicationModel.xml
FinalMergedApplicationModel=C:\scot\config\FinalMergedApplicationModel.xml
ACSIODLL=C:\ACS\bin\ACSIOClient.dll
ACSEXFUNCSDLL=C:\ACS\bin\ACSExposedFunctions.dll
DefaultContextExpiryTime=0
TBUsesPipeServer=0
ACSIOQueueEntries=500
ListenToDevices=54,40,67,30
ApplicationName=salesw2k.exe
AuxMsgInputSequence=FL_NOTIFY_ENTERED
AuxMsgProperty=PropFLNotifyData
AuxMsgIdProperty=PropFLNotifyId
AuxMsgTLOGOverrideEnabled=1

[Tracing]
TBTraceSize=5000000
TBTraceOn=Y
TBTraceFileName=C:\SCOT\bin\TBTrace.log
TBTraceMask=0xF7
TBTraceModuleMask=0xFF  <- masks: TBSOdll=0x08, SSF=0x04, PipeServer=0x02, TB=0x01
TBTraceTruncateAfter=1000

[Receipt]
ReceiptFilePath=c:\scot\data\receipts
POSPrinterDotsPerLine=24

[Check]
; CheckFrontTopAdjust baseline values: (7158 mode = 3), (7167 mode = 13)
; Finetune by +/- value.  Higher numbers will print check front higher on check.
; Value Range limits = 0 to 20
CheckFrontTopAdjust=13
SlipDetectCheckFront=Electronically Converted
SlipDetectCheckBack=FOR DEPOSIT ONLY
SlipDetectCheckDenial=DENIAL RECORD;RM06
SlipDetectEBT=EBT DECLINED
SlipDetectNegDept=Misc Credit;Commercial Charge
SlipDescCheckFront=Insert Check face up for printing (if not already inserted)
SlipDescCheckBack=Check endorsement required (if not already inserted)
SlipDescCheckDenial=Insert Check face down for Decline language print (if not already inserted)
SlipDescEBT=Insert EBT decline slip
SlipDescNegDept=Insert negative department slip

[Itemization]
CouponTypes=;6;7;10;11;12;13;
MonetaryRewards=;0;1;2;3;4;5;
PointRewards=;6;7;8;9;24;30;
AllowACSToIgnoreItemsOnResume=N

[Special Barcodes]
NeedInterventionBarcodes=070882018933

[ACS Tender Types]
CASH=1
CREDIT CARDS=2
ATM/DEBIT CARD=3
DEBIT TENDER=3
GIFT CARD=9
CHECK=40
GIFT/RETURN CARD=9
FOOD STAMP=8
MEIJER CREDIT CARD=20

[Tender Timeout]
ProcessCoinsAfterThisTimeOut=4

[SignatureCapture]
SigBmpSource=C:\TEMP\
SigBmpTarget=C:\scot\data\Signatures\
SigBmpFilePattern=PAM*.tmp

; the EFTResponseCodes section determine special handling
; for EFT response codes within TB.  Codes that are not found here and are not approved are declined. 
;
; Serious response code handling without suspending.
; The [Serious] section has been added. Place response codes in this section
; that you want the operator to handle (call for auth., stolen cards, etc)
;

[EFTResponseCodes]
ResponseCodeApprove=NL16,CC00,NL06,NL04,NL27,NL03,RM00,NL25,NL13

RCodeGiftCardRemainingBalance=RM74,NL36,NL32

RCodePhoneGiftDelayedActivation=NL33

;EBT disply nice message to user and let operator call for auth.
;Any EBT declines that require endorsement should be entered in this list!
RCodeCardRemainingBalance=NL35,RM49,RM70,RM47,NL32,RM51,NL12,RM96,RM06


RCodeSerious=RM37,RM62,DFLT,RM67,RM41,RM08,RM34,NL14


[TBFeatures]
; Configuration to turn on or turn off RAP Tendering
TB_FT_SUPPORT_RAPTENDER=Y

[EnableStoreModeButtons]
;Adding entry here meaning have corresponding modification in SSF 
DuplicateEFTReceipt=N
TrainingMode=Y

[POS Operation]
;This is created to temporarily disable the scrolling of item in POS. There are instances that the POS abnormally shutdown with this functionality
SendSelectedEntryID=Y


[POSKeyDefinitions]
VoidCurrentItem={F3}
Acknowledge={F1}
Cancel={F2}
ElectronicPayment={PAYMENT}{F4}
CancelTender={PAYMENT}{F7}
CancelTransaction={TRANS}{F3}
SuspendTransaction={TRANS}{F1}
TenderSelectedKey={F3}

[Print Signature]
;Y=when you press the print signature button, there is nothing to happen
;N=back to its default functionality
BypassButtonFuntionality=N

[ShowWaitOption]
;Y=Everytime FL switch into SMInprogress states, it displays "Processing Please wait screen".
;N=back to its default functionality of SMInprogress state
ShowPleaseWait=N

[MultiBitmapPrinting]
PrinterMultiBitmapDataFile=C:\scot\data\MultiBitmapData.xml
PrinterMultiBitmapEnabled=1
PrinterMultiBitmapSyncRAP=0

