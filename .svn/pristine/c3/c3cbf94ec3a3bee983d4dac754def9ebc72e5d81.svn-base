			<IncludeControl Name="LaneCaptionText"/>
			<IncludeControl Name="LaneCaption"/>
			<IncludeControl Name="TotalText"/>
			<IncludeControl Name="TotalAmount"/>	
			<IncludeControl Name="ItemText"/>
			<IncludeControl Name="ItemAmount"/>			
			<IncludeControl Name="TitleText">
				<TextFormat>$CheckOnLaneTitle$</TextFormat>
			</IncludeControl>					
			<IncludeControl Name="InstructionText">
				<TextFormat>$CheckOnLaneLeadthru$</TextFormat>
			</IncludeControl>		
		</Context>
		<Context Name="CheckOnLaneRap">
