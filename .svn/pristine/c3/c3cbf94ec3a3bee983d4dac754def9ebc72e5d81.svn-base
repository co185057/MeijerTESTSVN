<Display Version="1.0">
  <Includes>
    <Include File="%ConfigPath%\HHACSAssistModeSpecific.xml" />
  </Includes>
  <Controls>
    <Control Name="PrepaidCardList" Type="Receipt">
      <Properties>
        <Property Name="Position">20,120,217,136</Property>
        <Property Name="FillStyle">1</Property>
        <Property Name="Font">ReceiptFont</Property>
        <Property Name="TextColor">Black</Property>
        <Property Name="SelectionFont">ReceiptFont</Property>
        <Property Name="SelectionBackgroundColor">Gray</Property>
        <Property Name="SelectionForegroundColor">White</Property>
      </Properties>
      <CustomData>
        <Data Name="DataSource">PrepaidCardList</Data>
        <Data Name="ItemType1">ITEM_DESCRIPTION</Data>
        <Data Name="ItemType1StringID">$AMReceiptItemType2$</Data>
        <Data Name="ItemType1Wrap">True</Data>
      </CustomData>
    </Control>
  </Controls>
  <Contexts>
