[Tracing]
TBTraceFileName=c:\scot\bin\tbtrace.log
TBTraceMask=0xFF
TBTraceOn=Y
TBTraceModuleMask=0xFF
TBTraceTruncateAfter=30000
TBTraceUNICODE=N

[Operations]
TBDataEntryINI=C:\SCOT\config\TBScotDataEntry.ini
TBUsesPipeServer=Y
; Should postscotstop.bat be called before calling prescotstart again? Most solutions
; start an app that would need to be stopped before starting up again.
PreScotStartDependsOnPostScotStop=Y

;The Iso 639 standard language code of Primary language - TB uses to read txt from scudmsgs files
ISO639PrimaryLanguageCode=EN

; To enable shared use of the message files, they are specified here. These should not
; be altered without consultation with NCR
SCOTMessageFile=c:\\scot\\config\\scotmsge.dat
SCUDMessageFile=c:\\scot\\config\\scudmsgs.dat
SCUDMessage2File=c:\\scot\\config\\SCUDMSG2.dat

[TBFeatures]
;The following features can be turned on or off by altering the following options - features must be
;implemented in the TB before they function correctly
TB_FT_TENDER=Y
TB_FT_ITEMSALE=Y
TB_FT_SUSPEND=Y
TB_FT_DEBITACCOUNTTYPE=Y
TB_FT_EXTENDEDDETAILS=Y
TB_FT_TRAININGMODE=Y
TB_FT_QUICKITEMENTRY=N
TB_FT_ENDORSEMENT=Y
TB_FT_SECURITYLEVEL=Y
TB_FT_MICR=Y
TB_FT_CASHDRAWER=Y
TB_FT_AMSTATUS=Y
TB_FT_SUPPORT_PIM=Y



[TBxml]
MsgMapXML=C:\SCOT\config\TBMsgMap.xml
TBInProgressXML=C:\SCOT\config\TBProgressStates.xml

UnhandledMessageTimeoutInSeconds=8
UnhandledReturnCode=11

